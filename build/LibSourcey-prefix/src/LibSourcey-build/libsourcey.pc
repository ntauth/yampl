Name: LibSourcey
Description: LibSourcey C++ Networking Evolved
Version: 1.1.4

prefix=/usr/local
exec_prefix=/usr/local/bin
libdir=/usr/local/lib
includedir=/usr/local/include
vendorlibdir=/usr/local/share/libsourcey/vendor/lib
vendorincludedir=/usr/local/share/libsourcey/vendor/include

Requires:
Libs: -L${libdir} -L${vendorlibdir}  -lscy_base -lscy_pluga -llibuv -lzlib -lminizip -lhttp_parser
Cflags: -I${includedir} -I${vendorincludedir}
